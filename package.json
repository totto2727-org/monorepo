{
  "devDependencies": {
    "@biomejs/biome": "catalog:lint",
    "@getgrit/cli": "catalog:lint",
    "@tsconfig/strictest": "catalog:tsconfig",
    "@types/node": "catalog:dev",
    "@typescript/native-preview": "catalog:build",
    "dprint": "catalog:lint",
    "pncat": "catalog:cli",
    "turbo": "catalog:cli",
    "typescript": "catalog:build",
    "ultracite": "catalog:lint",
    "wrangler": "catalog:cloudflare"
  },
  "name": "monorepo",
  "packageManager": "pnpm@10.20.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@getgrit/cli",
      "@parcel/watcher",
      "@pulumi/docker",
      "@pulumi/docker-build",
      "@tailwindcss/oxide",
      "bufferutil",
      "core-js",
      "core-js-pure",
      "dprint",
      "esbuild",
      "msgpackr-extract",
      "pncat",
      "protobufjs",
      "sharp",
      "utf-8-validate",
      "workerd"
    ],
    "overrides": {
      "vite": "catalog:build"
    },
    "patchedDependencies": {
      "casbin@*": "patches/casbin@5.39.0.patch"
    }
  },
  "scripts": {
    "check": "${npm_execpath} run /check:.*/",
    "check:biome": "biome check",
    "check:dprint": "dprint check",
    "check:grit": "grit check",
    "fix": "${npm_execpath} run /fix:.*/",
    "fix:grit": "grit check --fix",
    "fix:biome": "biome check --write --unsafe",
    "fix:dprint": "dprint fmt --allow-no-files",
    "package": "turbo --filter @package/*",
    "pncat": "pncat",
    "precommit": "turbo fix check test build"
  },
  "type": "module"
}
