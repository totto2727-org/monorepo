{
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@tsconfig/node24": "catalog:",
    "@tsconfig/remix": "catalog:",
    "@tsconfig/strictest": "catalog:",
    "@types/node": "catalog:",
    "dprint": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "wrangler": "catalog:"
  },
  "name": "monorepo",
  "packageManager": "pnpm@10.11.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@clerk/shared",
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "core-js",
      "dprint",
      "esbuild",
      "msgpackr-extract",
      "sharp",
      "workerd"
    ]
  },
  "scripts": {
    "catalog": "nlx codemod pnpm/catalog",
    "check": "$npm_execpath run /check:.*/",
    "check:biome": "biome check",
    "check:dprint": "dprint check",
    "fix": "$npm_execpath run /fix:.*/",
    "fix:biome": "biome check --write --unsafe",
    "fix:dprint": "dprint fmt --allow-no-files",
    "precommit": "turbo fix && turbo check"
  }
}
