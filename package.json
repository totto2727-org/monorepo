{
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@tsconfig/node24": "^24.0.0",
    "@tsconfig/remix": "^2.0.5",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^24.1.0",
    "dprint": "^0.50.0",
    "pncat": "^0.4.2",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "wrangler": "4.30.0"
  },
  "name": "monorepo",
  "packageManager": "pnpm@10.11.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@clerk/shared",
      "@parcel/watcher",
      "@pulumi/docker",
      "@pulumi/docker-build",
      "@tailwindcss/oxide",
      "core-js",
      "core-js-pure",
      "dprint",
      "esbuild",
      "msgpackr-extract",
      "protobufjs",
      "sharp",
      "workerd"
    ]
  },
  "scripts": {
    "check": "$npm_execpath run /check:.*/",
    "check:biome": "biome check",
    "check:dprint": "dprint check",
    "fix": "$npm_execpath run /fix:.*/",
    "fix:biome": "biome check --write --unsafe",
    "fix:dprint": "dprint fmt --allow-no-files",
    "pncat": "nlx pncat",
    "precommit": "turbo fix && turbo check"
  },
  "type": "module"
}
