{
  "devDependencies": {
    "@biomejs/biome": "catalog:dev",
    "@cloudflare/vite-plugin": "catalog:dev",
    "@cloudflare/workers-oauth-provider": "catalog:dev",
    "@cloudflare/workers-types": "catalog:dev",
    "@formatjs/intl-durationformat": "catalog:dev",
    "@hono/mcp": "catalog:dev",
    "@hono/standard-validator": "catalog:dev",
    "@modelcontextprotocol/sdk": "catalog:dev",
    "@tailwindcss/cli": "catalog:dev",
    "@totto/function": "catalog:dev",
    "agents": "catalog:dev",
    "daisyui": "catalog:dev",
    "dprint": "catalog:dev",
    "drizzle-kit": "catalog:dev",
    "drizzle-orm": "catalog:dev",
    "hono": "catalog:dev",
    "tailwindcss": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "zod": "catalog:dev"
  },
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "imports": {
    "#@*": "./app*"
  },
  "name": "mcp",
  "private": true,
  "scripts": {
    "build": "vite build",
    "check": "tsc",
    "db:apply": "wrangler d1 migrations apply mcp",
    "db:generate": "drizzle-kit generate",
    "deploy": "wrangler deploy",
    "dev": "vite dev",
    "prebuild": "$npm_execpath run -p /prebuild:.*/",
    "prebuild:tailwindcss": "tailwindcss -i ./app/ui/layout/tailwind.css -o ./public/asset/tailwind.css",
    "prebuild:wrangler": "wrangler types",
    "preview": "wrangler dev --test-scheduled",
    "start": "wrangler dev --test-scheduled"
  },
  "type": "module"
}
