{
  "tasks": {
    "//#check": {},
    "//#fix": {},
    "build": {
      "dependsOn": ["prebuild"],
      "outputs": ["**/build/**", "**/dist/**"]
    },
    "check": {
      "dependsOn": ["prebuild"]
    },
    "deploy": {
      "dependsOn": ["build"]
    },
    "dev": {
      "dependsOn": ["prebuild"],
      "persistent": true
    },
    "fix": {},
    "prebuild": {
      "dependsOn": ["^build"],
      "outputs": ["**/worker-configuration.d.ts", "**/routeTree.gen.ts"]
    },
    "test": {
      "dependsOn": ["prebuild"]
    }
  }
}
